Var
  FI, FO : Text;
  N, i, j, k : Word;

BEGIN
  Assign(FI, TenTepDL);
  Reset(FI);
  ReadLn(FI, N);
  Assign(FO, TenTepKQ);
  Rewrite(FO);
  For i := 1 To N Do
    Begin
      For j := 1 To N Do
        Begin
          Read(FI, k);
          Write(FO, FX[k]:3);
        End;
      WriteLn(FO);
    End;
  Close(FO);
  Close(FI);
END.